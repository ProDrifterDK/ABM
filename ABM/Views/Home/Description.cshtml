@{
    ViewBag.Title = "Nuestra tienda";
}
<div class="container center-align">
    <div class="row">
        <h2 style="padding-bottom: 50px; padding-top:20px">Conozca nuestra tienda</h2>
    </div>
    <div class="row" style="padding-bottom:30px">
        <div class="mapouter">
            <div class="gmap_canvas"><iframe width="700" height="500" id="gmap_canvas" src="https://maps.google.com/maps?q=camilo%20henriquez%2092%2C%20curic%C3%B3&t=&z=17&ie=UTF8&iwloc=&output=embed" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"></iframe>Google Maps Generator by <a href="https://www.embedgooglemap.net">embedgooglemap.net</a></div>
            <style>
                .mapouter {
                    position: relative;
                    text-align: right;
                    height: 500px;
                    width: 600px;
                }

                .gmap_canvas {
                    overflow: hidden;
                    background: none !important;
                    height: 500px;
                    width: 700px;
                }
            </style>
        </div>
    </div>
    <div class="row center-align" style="padding-bottom:30px">
        <h4>Horario de atención</h4>
        <h5>Lunes a viernes de 10:00 a 18:00</h5>
        <h5>Sábados de 10:00 a 13:00</h5>
    </div>
</div>
